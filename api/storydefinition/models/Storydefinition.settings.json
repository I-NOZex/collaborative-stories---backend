{
  "connection": "default",
  "collectionName": "storydefinition",
  "info": {
    "name": "storydefinition",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "title": {
      "default": "",
      "type": "string"
    },
    "user": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "wordsPerUser": {
      "default": 250,
      "type": "integer",
      "max": 250
    },
    "storyblocks": {
      "collection": "storyblock",
      "via": "storydefinition"
    },
    "likes": {
      "default": 0,
      "type": "integer"
    }
  }
}